elasticsearch:    elasticsearch -D es.config=config/elasticsearch/elasticsearch.yml
redis:            redis-server
mongod:           mongod --config /usr/local/etc/mongod.conf --logappend -v --logpath log/mongo.log
memcached:        /usr/local/bin/memcached
grunt:            grunt && grunt watch
